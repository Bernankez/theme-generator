{
  "name": "@bernankez/theme-generator",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.5",
  "description": "Website theme generator",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "homepage": "https://github.com/Bernankez/theme-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/theme-generator.git"
  },
  "bugs": "https://github.com/Bernankez/theme-generator/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild && pnpm -F \"playground\" build",
    "dev": "unbuild --stub && pnpm -F \"playground\" dev",
    "fix": "eslint . -f mo --fix",
    "lint": "eslint . -f mo",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "preview": "pnpm -F \"playground\" preview",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit && pnpm -F \"playground\" typecheck"
  },
  "peerDependencies": {
    "unocss": "^0.59.4"
  },
  "peerDependenciesMeta": {
    "unocss": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@bernankez/eslint-config": "^1.3.2",
    "@bernankez/utils": "^0.6.3",
    "@types/chroma-js": "^2.4.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@unocss/eslint-plugin": "^0.59.4",
    "bumpp": "^9.4.0",
    "chroma-js": "^2.4.2",
    "eslint": "^9.1.1",
    "eslint-formatter-mo": "^1.2.0",
    "eslint-plugin-format": "^0.1.1",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unocss": "^0.59.4",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "nlx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
